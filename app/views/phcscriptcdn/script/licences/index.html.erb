<p id="notice"><%= notice %></p>

<h1>Script Licences</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @script_licences.each do |script_licence| %>
      <tr>
				<td><%= script_licence.scriptversion %></td>
        <td><%= link_to 'Show', script_licence %></td>
        <td><%= link_to 'Edit', edit_script_licence_path(script_licence) %></td>
        <td><%= link_to 'Destroy', script_licence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Script Licence', new_script_licence_path %>
