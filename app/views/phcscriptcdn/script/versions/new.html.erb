<!-- Title System -->
<% phc_title "Script Version" %>
<% phc_title_tagline "Add a Script Version Number" %>

<!-- Page Header -->
<div class="row">
	<div class="col-lg-12">

		<!-- Bread Crumbs -->
		<ol class="breadcrumb">
			<li><%= link_to "Script Listings Index", script_listings_path %></li>
			<li><%= link_to "Versions Index", script_versions_path %></li>
			<li class="active"><%= yield(:phc_title_tagline) %></li>
		</ol>

		<!-- Page Title -->
		<h3 class="page-title">
			<%= yield(:phc_title) %> </br>
			<small><%= yield(:phc_title_tagline) %></small>
		</h3>

	</div>
</div>

<!-- Main Content -->
<div class="row">
	<div class="col-lg-12">

		<%= render 'form', script_version: @script_version %>
		<%= link_to 'Back', script_versions_path, class: "btn btn-primary" %>

	</div>
</div>
