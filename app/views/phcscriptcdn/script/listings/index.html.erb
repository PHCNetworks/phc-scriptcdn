<p id="notice"><%= notice %></p>

<h1>Script Listings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @script_listings.each do |script_listing| %>
      <tr>
        <td><%= link_to 'Show', script_listing %></td>
        <td><%= link_to 'Edit', edit_script_listing_path(script_listing) %></td>
        <td><%= link_to 'Destroy', script_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Script Listing', new_script_listing_path %>
